<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yunitadwipj.antibiotikonline.MainActivity">


    <ImageButton
        android:id="@+id/alarm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/alarm"/>
    <ImageButton
        android:id="@+id/lonceng1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/lonceng"
        android:layout_weight="1"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/alarm"/>


    <ImageButton
        android:id="@+id/tips"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tips"
        android:layout_weight="1"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"/>
    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/tips"
        android:text="Tips"
        android:textSize="15dp"
        android:textAlignment="center"/>

    <ImageButton
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/status"
        android:layout_weight="1"
        android:layout_centerInParent="true"/>
    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_below="@+id/tips"
        android:text="Status"
        android:textSize="15dp"
        android:textAlignment="center"/>

    <ImageButton
        android:id="@+id/lemari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/lemari"
        android:layout_weight="1"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"/>
    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tips"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="Lemari Obat"
        android:textSize="15dp"
        android:textAlignment="center"/>

    <ImageButton
        android:id="@+id/dokterku"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/dokterku"
        android:layout_weight="1"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/tv1"/>
    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dokterku"
        android:text="Dokterku"
        android:textSize="15dp"
        android:textAlignment="center"/>

    <ImageButton
        android:id="@+id/darurat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/darurat"
        android:layout_weight="1"
        android:layout_centerInParent="true"
        android:layout_below="@+id/tv2"/>
    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dokterku"
        android:layout_centerInParent="true"
        android:text="Darurat"
        android:textSize="15dp"
        android:textAlignment="center"/>

    <ImageButton
        android:id="@+id/bantuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bantuan"
        android:layout_weight="1"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_below="@+id/tv3"/>
    <TextView
        android:id="@+id/tv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/dokterku"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:text="Bantuan"
        android:textSize="15dp"
        android:textAlignment="center"/>



    <TextView
        android:id="@+id/quotes"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#66CC99"
        android:layout_below="@+id/tv5"
        android:layout_centerHorizontal="true"
        android:text="QUOTES"/>



</RelativeLayout>




